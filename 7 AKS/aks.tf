module "aks" {
      source                           = "./modules/aks"
      cluster_name                     = "
      kubernetes_version               = "1.25.6"
      orchestrator_version             = "1.25.6"
      location                         = var.deploy_region
      rg_name                          = "${data.azurerm_resource_group.rg_name_AKS.name}"
      dns_prefix                       = ""      
      sku_tier                         = "Paid" 
      private_cluster_enabled          = "false"
      node_pool_name                   = "ehcdevaksnp" 
      vm_size                          = "standard_D4s_v4"
      os_disk_type                     = "Managed"
      vnet_subnet_id                   = "${data.azurerm_subnet.snet_aks.id}"
      enable_auto_scaling              = "false"
      node_count                       = "3"
      enable_node_public_ip            = "false"
      node_availability_zones          = ["1","2","3"]  
      node_type                        = "VirtualMachineScaleSets"
      node_max_pods                    =  "100"
      enable_host_encryption           =  "false"
      idtype                           =  "SystemAssigned" 
      enable_http_application_routing  =  "true"
      enable_kube_dashboard            =  "false"
      enable_azure_policy              =  "true"
      os_disk_size_gb                  =  "60"
      resource_name                    = "AzureKubernetesCluster"
      tenant_id                        = ""
      admin_group_object_id            = [""]
      project                          = var.project
	datecreated                      = local.timestamp
	env                              = var.env
      acr_scope                        = data.azurerm_container_registry.acr_name.id
      log_analtics_workspace_id        = ""
}